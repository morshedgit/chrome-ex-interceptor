{
  "manifest_version": 3,
  "name": "Interceptor Extension",
  "version": "1.0",
  "description": "This is an Interceptor extension",
  "icons": {
    "16": "16.png"
  },
  "permissions": ["storage", "scripting", "activeTab"],
  "host_permissions": ["https://*/*", "http://*/*"],
  "action": {
    "default_title": "Interceptor Extension",
    "default_icon": "16.png",
    "default_popup": "index.html"
  }
}
